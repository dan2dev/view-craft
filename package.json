{
  "name": "view-craft",
  "private": false,
  "version": "0.1.7",
  "type": "module",
  "main": "./dist/view-craft.cjs",
  "module": "./dist/view-craft.js",
  "browser": "./dist/view-craft.umd.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/view-craft.js",
      "require": "./dist/view-craft.cjs"
    },
    "./types": {
      "types": "./types/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && rollup -c && tsc --emitDeclarationOnly",
    "dev": "rm -rf dist && rollup -c -w & tsc --emitDeclarationOnly --watch",
    "dev:all": "pnpm dev & cd examples/basic && npm run dev",
    "build:watch": "rm -rf dist && rollup -c -w & tsc --emitDeclarationOnly --watch",
    "build:rollup": "rollup -c",
    "build:types": "tsc --emitDeclarationOnly",
    "clean": "rm -rf dist *.tsbuildinfo",
    "test": "vitest run --coverage",
    "test:watch": "vitest --watch"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/node": "^24.5.2",
    "jsdom": "^27.0.0",
    "rollup": "^4.52.0",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.17.0",
  "dependencies": {
    "type-fest": "^5.0.1"
  }
}
