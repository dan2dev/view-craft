{
  "name": "view-craft",
  "private": false,
  "version": "0.1.7",
  "type": "module",
  "main": "./dist/view-craft.cjs",
  "module": "./dist/view-craft.js",
  "browser": "./dist/view-craft.umd.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/view-craft.js",
      "require": "./dist/view-craft.cjs"
    },
    "./types": {
      "types": "./types/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && rollup -c && tsc --emitDeclarationOnly",
    "dev": "rm -rf dist && rollup -c -w & tsc --emitDeclarationOnly --watch",
    "build:watch": "rm -rf dist && rollup -c -w & tsc --emitDeclarationOnly --watch",
    "build:rollup": "rollup -c",
    "build:types": "tsc --emitDeclarationOnly",
    "clean": "rm -rf dist *.tsbuildinfo",
    "test": "vitest run"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.1.4",
    "jsdom": "^26.1.0",
    "rollup": "^4.47.1",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.15.0+sha512.486ebc259d3e999a4e8691ce03b5cac4a71cbeca39372a9b762cb500cfdf0873e2cb16abe3d951b1ee2cf012503f027b98b6584e4df22524e0c7450d9ec7aa7b",
  "dependencies": {
    "type-fest": "^4.41.0"
  }
}