{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "typeRoots": [
      "./types",
      "./node_modules/@types"
    ],
  "declaration": true,
  "outDir": "./dist",
  },
  "include": [
    "src",
    "types"
  ]
}
